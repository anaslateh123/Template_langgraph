# LangGraph Agent Template Environment Variables

# LLM Configuration
LLM_MODEL_NAME=gpt-4
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=4000

# Memory Database Configuration
MEMORY_DB_CONNECTION_STRING=postgresql://postgres:postgres@localhost:5432/memory_db
MEMORY_DB_POOL_SIZE=5

# External Services (comma-separated list)
EXTERNAL_SERVICES_ENDPOINTS=https://api-service1.example.com,https://api-service2.example.com

# Application Configuration
ENVIRONMENT=development
DEBUG=true

# API Keys (add your actual keys here)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
